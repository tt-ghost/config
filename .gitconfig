[core]
  excludesfile = /Users/xxx/.gitignore_global
  filemode = true
  bare = false
  logallrefupdates = true
[user]
	name = yourName
	email = yourName
[alias]
	ci = commit
	co = checkout
	br = branch
	st = status 
[push]
	default = matching
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[color]
	ui = true
